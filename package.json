{
  "name": "n8n-nodes-fetchai",
  "version": "1.0.0",
  "description": "n8n community node for Fetch.ai blockchain - uAgents, Almanac, DeltaV, Agentverse, CosmWasm, and more",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "fetchai",
    "fetch.ai",
    "blockchain",
    "uagents",
    "autonomous-agents",
    "almanac",
    "deltav",
    "agentverse",
    "cosmwasm",
    "cosmos",
    "ai-agents",
    "web3",
    "crypto",
    "FET",
    "ASI"
  ],
  "license": "BUSL-1.1",
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-fetch.ai",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-fetch.ai.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lint:fix": "eslint nodes credentials package.json --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/FetchaiNetwork.credentials.js",
      "dist/credentials/FetchaiApi.credentials.js",
      "dist/credentials/Almanac.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Fetchai/Fetchai.node.js",
      "dist/nodes/Fetchai/FetchaiTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0"
  },
  "dependencies": {
    "@cosmjs/amino": "^0.32.2",
    "@cosmjs/cosmwasm-stargate": "^0.32.2",
    "@cosmjs/crypto": "^0.32.2",
    "@cosmjs/encoding": "^0.32.2",
    "@cosmjs/math": "^0.32.2",
    "@cosmjs/proto-signing": "^0.32.2",
    "@cosmjs/stargate": "^0.32.2",
    "@cosmjs/tendermint-rpc": "^0.32.2",
    "axios": "^1.6.2",
    "bech32": "^2.0.0",
    "bip39": "^3.1.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
